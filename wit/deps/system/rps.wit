package profiling:system@0.0.0;

interface rps {
    use cpu.{cpu-mask};

    record rps-queue {
        name: string,
        cpus: option<cpu-mask>,
        flow-cnt: option<u32>,
    }

    record rps-info {
        device: string,
        queues: list<rps-queue>,
    }

    get-rps-info: func() -> list<rps-info>;
}

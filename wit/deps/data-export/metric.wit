package profiling:data-export@0.0.0;

interface metric {
    use types.{field-value};

    export-sample: func(sample: sample) -> result<_, string>;

    record sample {
        name : string,
        tags : list<tuple<string, string>>,
        value: field-value,
        ns-ts: option<u64>, // UNIX timestamp in nano seconds
    }
}
